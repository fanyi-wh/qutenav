<node name="updater">
    <interface name="net.kvanttiapina.qutenav.Updater">
        <method name="sync"/>
        <method name="ping">
          <arg name="pong" direction="out" type="s"/>
        </method>
        <signal name="ready"/>
    </interface>
</node>
